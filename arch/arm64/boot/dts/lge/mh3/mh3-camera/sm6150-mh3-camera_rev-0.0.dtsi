&pm6150l_flash0 {
	status = "ok";
	qcom,max-current = <700>;
	qcom,current-ma = <700>;
	qcom,duration-ms = <500>;
};

&pm6150l_flash1 {
	status = "ok";
	qcom,max-current = <700>;
	qcom,current-ma = <700>;
	qcom,duration-ms = <500>;
};

&pm6150l_flash2 {
	status = "disabled";
};

&pm6150l_torch0 {
	status = "ok";
	qcom,max-current = <100>;
	qcom,current-ma = <100>;
};

&pm6150l_torch1 {
	status = "ok";
	qcom,max-current = <100>;
	qcom,current-ma = <100>;
};

&pm6150l_torch2 {
	status = "disabled";
};

&pm6150l_switch0 {
	status = "ok";
};

&pm6150l_switch1 {
	status = "disbled";
};

&tlmm{
	cam_sensor_rear_n_active: cam_sensor_rear_n_active {
		mux {
			pins = "gpio47";
			function = "gpio";
		};

		config {
			pins = "gpio47";
			bias-disable;
			drive-strength = <2>;
		};
	};

	cam_sensor_rear_n_suspend: cam_sensor_rear_n_suspend {
		mux {
			pins = "gpio47";
			function = "gpio";
		};

		config {
			pins = "gpio47";
			bias-pull-down;
			drive-strength = <2>;
			output-low;
		};
	};

	cam_sensor_rear_w_active: cam_sensor_rear_w_active {
		mux {
			pins = "gpio37";
			function = "gpio";
		};

		config {
			pins = "gpio37";
			bias-disable;
			drive-strength = <2>;
		};
	};

	cam_sensor_rear_w_suspend: cam_sensor_rear_w_suspend {
		mux {
			pins = "gpio37";
			function = "gpio";
		};

		config {
			pins = "gpio37";
			bias-pull-down;
			output-low;
			drive-strength = <2>;
		};
	};

	cam_sensor_rear_d_active: cam_sensor_rear_d_active {
		mux {
			pins = "gpio45", "gpio24", "gpio25";
			function = "gpio";
		};

		config {
			pins = "gpio45", "gpio24", "gpio25";
			bias-disable;
			drive-strength = <2>;
			output-low;
		};
	};

	cam_sensor_rear_d_suspend: cam_sensor_rear_d_suspend {
		mux {
			pins = "gpio45", "gpio24", "gpio25";
			function = "gpio";
		};

		config {
			pins = "gpio45", "gpio24", "gpio25";
			bias-pull-down;
			drive-strength = <2>;
			output-low;
		};
	};

	cam_sensor_front_active: cam_sensor_front_active {
		mux {
			pins = "gpio40", "gpio24", "gpio25";
			function = "gpio";
		};

		config {
			pins = "gpio40", "gpio24", "gpio25";
			bias-disable;
			drive-strength = <2>;
			output-low;
		};
	};

	cam_sensor_front_suspend: cam_sensor_front_suspend {
		mux {
			pins = "gpio40", "gpio24", "gpio25";
			function = "gpio";
		};

		config {
			pins = "gpio40", "gpio24", "gpio25";
			bias-pull-down;
			drive-strength = <2>;
			output-low;
		};
	};

	cam_sensor_front_dvdd: cam_sensor_front_dvdd {
		mux {
			pins = "gpio98";
			function = "gpio";
		};

		config {
			pins = "gpio98";
			bias-disable;
			output-high;
			drive-strength = <2>;
		};
	};

	camera_rear_w_avdd: camera_rear_w_avdd {
		mux {
			pins = "gpio36";
			function = "gpio";
		};

		config {
			pins = "gpio36";
			bias-disable;
			output-high;
			drive-strength = <2>;
		};
	};

	camera_rear_w_dvdd: camera_rear_w_dvdd {
		mux {
			pins = "gpio38";
			function = "gpio";
		};

		config {
			pins = "gpio38";
			bias-disable;
			output-high;
			drive-strength = <2>;
		};
	};
